<div class="content">
  <h1>{{title}}</h1>
  <div class="plants-showcase">
    {{#each this.data}}
    <div class="plant-card">
      <h3>{{this.commonName}}</h3>
      <img class="plant-img" src="{{this.img}}">
      <a href="/plants/{{this._id}}">
        <button class="btn btn-round">+</button>
      </a>
    </div>
    {{/each}}
  </div>
</div>